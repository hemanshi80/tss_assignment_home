package com.tss.test;

import com.tss.model.IFactorial;

public class FactorialTest {

	public static void main(String[] args) {
		
		IFactorial factorial = (n)-> {
			int fact = 1;
			for (int i=1 ; i <= n; i++) {
				fact *=i;
			}
			return fact;
	};
		
	System.out.println("Factorial of 7 :" + (factorial.Factorial(7)));
			
	}

}
