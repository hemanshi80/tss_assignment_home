package com.tss.creational.factory.model;

import com.tss.creational.abstractfactory.model.ICar;

public class CarFactory {
	
	public ICar makeCars(CarType car){
		if (car == CarType.Maruti)
		{
			return new Maruti();
		}
		
		if (car == CarType.Mahindra) {
			return new Mahindra();
		}
		
		if (car == CarType.Tata) {
			return new Tata();
		}
		
		return null;
		
	}

}
